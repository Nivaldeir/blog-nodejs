<%- include ('../../partials/header.ejs') %>
<%- include ('../../partials/navbar.ejs') %>

<div class="container">
    <br>
    <h2>Usuarios</h2>
    <a href="/admin/users/create" class="btn btn-success">Cadastrar usuario</a>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user =>{%>
                <tr>
                    <th><%= user.id%></th>
                    <th><%= user.email%></th>
                </tr>
            <% })%>
        </tbody>
    </table>
</div>
<%- include ('../../partials/footer.ejs') %>